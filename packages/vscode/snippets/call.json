{
    "call.register": {
        "prefix": "rwr-call",
        "description": "rwr call register",
        "body": [
            "<call file=\"$1\" />",
            "$2"
        ]
    },
    "call.define": {
        "prefix": "rwr-call-define",
        "description": "rwr call define",
        "body": [
            "<call",
            "\tname=\"$1\"",
            "\tkey=\"$2\"",
            "\tradio_view_text=\"$3\"",
            "\tinitiation_comment1=\"$4\"",
            "\tacknowledge_comment=\"$5\"",
            "\tacknowledge_time=\"$6\"",
            "\tlaunch_comment=\"$7\"",
            "\tnotify_metagame=\"$8\"",
            ">",
            "$9",
            "</call>",
            "$10"
        ]
    },
    "call.round": {
        "prefix": "rwr-call-round",
        "description": "rwr call round",
        "body": [
            "<round",
            "\tlaunch_time=\"$1\"",
            "\tspawn_time=\"$2\"",
            ">",
            "$3",
            "</round>",
            "$4"
        ]
    },
    "call.effect": {
        "prefix": "rwr-call-effect",
        "description": "rwr call description",
        "body": [
            "<effect class=\"$1\" ref=\"$2\" />",
            "$3"
        ]
    }
}
