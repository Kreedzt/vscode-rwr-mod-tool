{
    "name": "vscode-rwr-mod-tool-root",
    "displayName": "RWR Mod Tool",
    "description": "Running with Rifles game mod tool",
    "license": "MIT",
    "repository": {
      "url": "https://github.com/Kreedzt/vscode-rwr-mod-tool"
    },
    "scripts": {
      "package": "pnpm run --filter vscode-rwr-mod-tool package",
      "publish": "pnpm run --filter vscode-rwr-mod-tool publish",
      "compile": "pnpm run --filter vscode-rwr-mod-tool compile",
      "pretest": "pnpm run compile && pnpm run lint",
      "format": "prettier --write \"**/*.ts\"",
      "test": "pnpm run --filter vscode-rwr-mod-tool test"
    },
    "devDependencies": {
      "@types/glob": "^8.1.0",
      "@types/mocha": "^10.0.1",
      "@types/node": "20.2.5",
      "@typescript-eslint/eslint-plugin": "^5.59.8",
      "@typescript-eslint/parser": "^5.59.8",
      "eslint": "^8.41.0",
      "glob": "^8.1.0",
      "mocha": "^10.2.0",
      "typescript": "^5.1.3"
    }
  }
  