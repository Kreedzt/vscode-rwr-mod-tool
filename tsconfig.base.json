{
	"compilerOptions": {
		"module": "commonjs",
		"target": "ES2020",
		"outDir": "out",
		"lib": [
			"ES2020"
		],
		"sourceMap": true,
		"composite": true,
		"declaration": true,
		"strict": true,
		"skipLibCheck": true,
		"noUnusedLocals": true,
		"noUnusedParameters": true,
		"baseUrl": "./",
		"paths": {
			"@angelscript/language-server": [
				"packages/angelscript-language-server/src"
			],
		},
	}
}
